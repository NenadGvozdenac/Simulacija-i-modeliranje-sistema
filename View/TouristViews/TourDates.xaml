<Window x:Class="BookingApp.View.TouristViews.TourDates"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookingApp.View.TouristViews"
        mc:Ignorable="d"
        Title="TourDates" Height="225" Width="400">
    <Grid>
        <DataGrid Name="tourDatesDataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Dates" Binding="{Binding Time}" Width="*" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
